1 задание
#include <iostream>
using namespace std;
int main() {
	int a, b, c;
	cin >> a >> b;
	c = a;
	a = b;
	b = c;
	cout << a << ' ' << b;
	return 0;
}
2 задание
#include <iostream>
using namespace std;
int main() {
	int a, b, c, d;
	cin >> a >> b >> c;
	d = a;
	a = c;
	c = b;
	b = d;
	cout << a << ' ' << b << ' ' << c;
	return 0;
}
